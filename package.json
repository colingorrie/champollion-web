{
  "name": "champollion-web",
  "author": "Colin Gorrie <colin@colingorrie.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/axios": "0.14.0",
    "axios": "0.18.0",
    "register-service-worker": "1.5.2",
    "vue": "2.5.17",
    "vue-class-component": "6.0.0",
    "vue-property-decorator": "7.0.0",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/preset-typescript": "7.1.0",
    "@metahub/cz-conventional-commit": "3.0.2",
    "@types/jest": "23.3.13",
    "@typescript-eslint/eslint-plugin": "1.1.0",
    "@typescript-eslint/parser": "1.1.0",
    "@vue/cli-plugin-babel": "3.2.0",
    "@vue/cli-plugin-e2e-cypress": "3.2.0",
    "@vue/cli-plugin-pwa": "3.2.0",
    "@vue/cli-plugin-typescript": "3.2.0",
    "@vue/cli-plugin-unit-jest": "3.2.0",
    "@vue/cli-service": "3.2.0",
    "@vue/test-utils": "1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "commitizen": "3.0.5",
    "commitlint": "7.5.2",
    "eslint": "5.12.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "node-sass": "4.9.0",
    "prettier": "1.16.1",
    "sass-loader": "7.0.1",
    "tailwindcss": "0.7.3",
    "ts-jest": "23.10.5",
    "typescript": "3.2.4",
    "typescript-eslint-parser": "22.0.0",
    "vue-template-compiler": "2.5.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@metahub/cz-conventional-commit",
      "cz-conventional-commit": {
        "emoji": true
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
